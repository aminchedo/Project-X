# Backend Environment Variables Example
# Copy this file to .env and fill in your actual values

# Database
DATABASE_URL=postgresql://username:password@localhost:5432/hts_trading

# Trading API Keys
BINANCE_API_KEY=your_binance_api_key_here
BINANCE_SECRET_KEY=your_binance_secret_key_here
KUCOIN_API_KEY=your_kucoin_api_key_here
KUCOIN_SECRET_KEY=your_kucoin_secret_key_here
KUCOIN_PASSPHRASE=your_kucoin_passphrase_here

# Block Explorer API Keys
TRONSCAN_API_KEY=your_tronscan_api_key_here
BSCSCAN_API_KEY=your_bscscan_api_key_here
ETHERSCAN_API_KEY=your_etherscan_api_key_here
ETHERSCAN_API_KEY_2=your_etherscan_backup_api_key_here

# Market Data API Keys
COINMARKETCAP_API_KEY=your_coinmarketcap_api_key_here
COINMARKETCAP_API_KEY_2=your_coinmarketcap_backup_api_key_here
CRYPTOCOMPARE_API_KEY=your_cryptocompare_api_key_here

# News API Keys
NEWSAPI_KEY=your_newsapi_key_here

# HuggingFace API Key (for AI analysis)
HUGGINGFACE_API_KEY=your_huggingface_api_key_here

# JWT Settings
JWT_SECRET_KEY=your_very_long_random_secret_key_here
JWT_ALGORITHM=HS256
JWT_EXPIRATION_HOURS=24

# Telegram Bot (optional)
TELEGRAM_BOT_TOKEN=your_telegram_bot_token_here
TELEGRAM_CHAT_ID=your_telegram_chat_id_here

# Development Settings
DEBUG=true
LOG_LEVEL=INFO

# CORS Settings
CORS_ORIGINS=http://localhost:5173,http://localhost:3000